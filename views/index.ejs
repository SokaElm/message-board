<h1><%=title%></h1>

<a href="<%= links[1].href %>">
  <span> Send New Message</span>
  <br />
</a>

<ul>
  <% for (let i = 0; i < messages.length; i++) { %>
  <li>
    From: <%= messages[i].username %>
    <a href="<%= `${links[2].href}/${messages[i].id}`%>">
      <span>Open</span>
    </a>
    <br />
    <% }; %>
  </li>
</ul>
